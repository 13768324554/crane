{
  "Services": {
    "mysql": {
      "Name": "mysql",
      "Labels": {
        "name": "mysql"
      },
      "TaskTemplate": {
        "ContainerSpec": {
          "Image": "mysql",
          "Labels": {
            "name": "mysql"
          },
          "Env": [
            "MYSQL_ROOT_PASSWORD=foobar"
            ]
        },
        "Resources": {
          "Limits": {
            "NanoCPUs": 10000000,
            "MemoryBytes": 4194304
          },
          "Reservations": {
            "NanoCPUs": 10000000,
            "MemoryBytes": 4194304
          }
        },
        "RestartPolicy": {
          "Condition": "none",
          "Delay": 3000000000,
          "MaxAttempts": 1,
          "Window": 3000000000
        },
        "Placement": {
          "Constraints": []
        },
        "LogDriver": {
          "Name": "json-file"
        }
      },
      "Mode": {
        "Replicated": {
          "Replicas": 1
        }
      },
      "UpdateConfig": null,
      "EndpointSpec": {
        "Mode": "vip",
        "Ports": [
          {
            "Name": "testport",
            "Protocol": "tcp",
            "TargetPort": 3306,
            "PublishedPort": 3306
          }
        ]
      }
    }
  }
}


