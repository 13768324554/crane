{
    "Services": {
        "mysql": {
            "Name": "mysql",
                "Labels": {
                    "name": "mysql"
                },
                "TaskTemplate": {
                    "ContainerSpec": {
                        "Image": "mysql",
                        "Labels": {
                            "name": "mysql"
                        },
                        "Env": [
                            "MYSQL_ROOT_PASSWORD=wordpress",
                            "MYSQL_PASSWORD=wordpress",
                            "MYSQL_USER=wordpress",
                            "MYSQL_DATABASE=wordpress"
                        ],
                        "User": "root"
                    },
                    "RestartPolicy": {
                        "Condition": "none",
                        "Delay": 3000000000,
                        "MaxAttempts": 1,
                        "Window": 3000000000
                    }
                },
                "Mode": {
                    "Replicated": {
                        "Replicas": 1
                    }
                },
                "UpdateConfig": null,
                "Networks": [
                    "68xppodqyxrs2f8w5jai9y8b3"
                ],
                "EndpointSpec": {
                    "Mode": "vip",
                    "Ports": [{
                        "Name": "testport",
                        "Protocol": "tcp",
                        "TargetPort": 3306,
                        "PublishedPort": 3306
                    }]
                }
        },
        "workpress": {
            "Name": "wordpress",
            "Labels": {
                "wordpress": "wordpress"
            },
            "TaskTemplate": {
                "ContainerSpec": {
                    "Image": "wordpress",
                    "Labels": {
                        "name": "wordpress"
                    },
                    "Env": [
                        "WORDPRESS_DB_HOST=cli_mysql:3306",
                        "WORDPRESS_DB_PASSWORD=wordpress"
                    ],
                    "User": "root"
                },
                "RestartPolicy": {
                    "Condition": "none",
                    "Delay": 3000000000,
                    "MaxAttempts": 1,
                    "Window": 3000000000
                }
            },
            "Mode": {
                "Replicated": {
                    "Replicas": 1
                }
            },
            "UpdateConfig": null,
            "Networks": [
                "68xppodqyxrs2f8w5jai9y8b3"
            ],
            "EndpointSpec": {
                "Mode": "vip",
                "Ports": [{
                    "Name": "testport",
                    "Protocol": "tcp",
                    "TargetPort": 80,
                    "PublishedPort": 8080
                }]
            }
        }
    },
    "Version": "1"
}
